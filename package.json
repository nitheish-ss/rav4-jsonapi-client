{
  "name": "rav4-jsonapi-client",
  "version": "1.0.0",
  "description": "JSON API data provider for react-admin, V4 Style, Support for SAFRS, ApiLogicServer",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/ra-jsonapi-provider.ts",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitheish-ss/rav4-jsonapi-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nitheish-ss/rav4-jsonapi-client/issues"
  },
  "homepage": "https://github.com/nitheish-ss/rav4-jsonapi-client#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^4.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "query-string": "^7.1.1",
    "react-admin": "^4.4.1",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.4",
    "eslint-plugin-prettier": "^4.2.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-check": "^6.0.1",
    "react": "^18.2.0"
  }
